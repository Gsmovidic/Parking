@model Parking.Data.Entities.Cliente
@{
    ViewData["Title"] = "Details";
}

<link rel="stylesheet" href="https://cdn.datatables.net/1.10.19/css/jquery.dataTables.min.css" />
<h1>Detalles del cliente</h1>
<div>
    <partial name="_ClienteDetails" />
</div>
<p>
    
    <a asp-action="AgregarVehiculo" asp-route-id="@Model?.Id" class="btn btn-outline-warning"><i class="fa-solid fa-car"></i></i></a>
    <a asp-action="Detalles" asp-route-id="@Model?.Id" class="btn btn-outline-info"><i class="fa-solid fa-person"></i></a>
    <a asp-action="Index" class="btn btn-success mt"><i class="fa-solid fa-arrow-left"></i></a>
</p>
<div class="row">
    <div class="col-md-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title">Vehiculos</h3>
            </div>
           
        </div>
    </div>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
    <script src="//cdn.datatables.net/1.10.19/js/jquery.dataTables.min.js"></script>

    <script type="text/javascript">
        $(document).ready(function () {
            $('#MyTable').DataTable({

                "aLengthMenu": [
                    [25, 50, 100, 200, -1],
                    [25, 50, 100, 200, "Todos"]
                ]
            });
        });
    </script>
}